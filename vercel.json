{
    "version": 2,
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "ALLOW-FROM https://squealing-cent.surge.sh"  // Permite incrustar desde un dominio espec√≠fico
          },
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors 'self' https://squealing-cent.surge.sh"  // Alternativa moderna a X-Frame-Options
          }
        ]
      }
    ]
  }